{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "semi": [2, "never"],
        "comma-dangle": 0,
        "no-console": 0,
        "quotes": [2, "single"],
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "vue/multi-word-component-names": 0
    }
}
