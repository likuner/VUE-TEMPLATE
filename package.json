{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "a template project based on vue3 & webpack5",
  "scripts": {
    "start": "npm run serve",
    "serve": "cross-env NODE_ENV=development BASE_PATH=/ webpack serve",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress",
    "dev": "cross-env NODE_ENV=development webpack --watch --progress"
  },
  "keywords": [
    "vue",
    "vue3",
    "template",
    "webpack5"
  ],
  "author": "likun",
  "license": "ISC",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "dependencies": {
    "@likun./lazy-img": "^1.0.18",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.9",
    "babel-loader": "^8.2.4",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.3",
    "dotenv-webpack": "^7.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "mockjs": "^1.1.0",
    "node-sass": "^7.0.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.6.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.0"
  }
}
